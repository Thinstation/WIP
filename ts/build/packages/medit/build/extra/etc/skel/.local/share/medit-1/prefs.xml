<?xml version="1.0" encoding="UTF-8"?><!-- -*- xml -*- -->
<moo-prefs version="1.0">
  <Editor>
    <langs>
      <lang id="ini">
        <globs>*.ini;*.conf.example;*.conf</globs>
      </lang>
      <lang id="pkgconfig">
        <globs>*.pc;*.buildtime;*.user;*.sample</globs>
      </lang>
      <lang id="diff">
        <config>strip: false</config>
      </lang>
      <lang id="makefile">
        <config>use-tabs: true</config>
      </lang>
    </langs>
  </Editor>
</moo-prefs>
